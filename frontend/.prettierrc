{
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
